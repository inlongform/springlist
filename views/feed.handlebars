                <div class="column col-sm-3 col-xs-3" id="sidebar">
                    <ul class="nav">
                        <li>
                            <div class="row">
                                <div class="col-xs-9">
                                    <a href="/feed/{{me.id}}">
                                        <img src="{{ me.picture }}" class="img-circle">
                                        <span>{{ me.name }}</span>
                                    </a>
                                </div>
                            </div>
                        </li>
                        
                        
                    </ul>
                </div>
                <div class="column col-sm-9" id="main">
                    <div class="col-sm-12" id="featured">
                        <div class="page-header text-muted" uid="{{user.id}}">
                            <span>{{ user.name }}</span>
                            <a class="glyphicon glyphicon-plus scrape"></a>
                            {{#if isUser}}
                            {{else}}
                                <a class="glyphicon glyphicon-user get_friends_byId"></a>
                            {{/if}}
                            <form action="/search" method="post" id="form_hash">
                                <div class="input-group input-group-sm">
                                  <span class="input-group-addon">#</span>
                                  <input type="text" class="form-control hash_field" placeholder="search">
                                </div>
                                <a class="glyphicon glyphicon-search search_hash"></a>
                            </form>

                        </div>
                    </div>

                    <div class="full col-sm-9 col-xs-9" id="follow_results"></div>

                    <div class="full col-sm-9 col-xs-9" id="feed">
                        <!-- content -->

                        {{#each data}}
                            <div class="row">
                                <div class="col-sm-10 col-xs-12 news-item" data-id="{{_id}}">

                                    <div class="media">
                                    {{#if picture}}
                                        <a href="{{link}}" target="_blank">
                                            <div class="media_wrapper">
                                                <img src="{{picture}}" alt="" class="media-object img-rounded img-responsive pull-left">
                                            </div>
                                        </a>
                                    {{/if}}
                                        <a href="{{link}}" target="_blank"><h4 class="media-heading">{{name}}</h4></a>
                                        <p class="date">{{created_time}}</p>
                                        <p class="description">{{{description}}}</p>

                                        <p class="hashes">
                                            {{#each hash}}
                                                <a href="/search/{{this}}">#{{this}}</a>,
                                            {{/each}}
                                        </p>
                                    </div>
                                </div>
                                <div class="col-sm-2 col-xs-12">
                                    <ul class="push">

                                    {{#if ../me.facebook }}
                                        <li>
                                            <input type="checkbox" class="fb">
                                            <img src="/images/fb_icon.png" alt="">
                                        </li>
                                    {{/if}}
                                    {{#if ../me.twitter }}
                                        <li>
                                            <input type="checkbox" class="tw">
                                            <img src="/images/twitter_icon.png" alt="">
                                        </li>
                                    {{/if}}

                                    {{#if ../me.tumblr }}
                                        <li>
                                            <input type="checkbox" class="tu">
                                            <img src="/images/tumblr_icon.png" alt="">
                                        </li>
                                    {{/if}}
                                    {{#if ../me.hasEmail }}
                                        <li>
                                            <input type="checkbox" class="email">
                                            <img src="/images/email.png" alt="">
                                        </li>
                                    {{/if}}
                                    {{#if ../isUser}}
                                        <li>
                                            <button type="submit" class="btn btn-success btn-post">POST</button>
                                        </li>
                                    {{else}}
                                        <li>
                                            <button type="submit" class="btn btn-success btn-add">ADD</button>
                                        </li>
                                    {{/if}}
                                    </ul>
                                </div>
                            </div>
                            <div class="row divider">
                                <div class="col-sm-12 col-xs-12">
                                    <hr>
                                </div>
                            </div>
                        {{/each}}



