                        {{#if none}}
                        <div class="row" id="no_results">
                            <div class="col-md-12">
                                <h5>No Results</h5>
                            </div>
                        </div>
                        {{else}}
                            {{#each data}}
                            <div class="row">
                                <div class="col-md-11 col-sm-11 col-xs-10">

                                    <div class="media">
                                            <img src="{{picture}}" alt="" class="media-object img-responsive pull-left img-circle">
                                            <h5>{{name}}</h5>
                                            
                                        </div>
                                </div>

                                <div class="col-md-1 col-sm-1 col-xs-2">

                                {{#if isFollowing}}
                                    <button type="submit" class="btn btn-default pull-right follow" uid="{{uid}}">follow</button>
                                {{else}}

                                {{/if}}
                                </div>
                            </div>
                            <div class="row divider">
                                <div class="col-sm-12 col-xs-12">
                                    <hr>
                                </div>
                            </div>
                            {{/each}}
                        {{/if}}